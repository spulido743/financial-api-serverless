					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200, 400, or 404\", function () {",
									"    pm.expect(pm.response.code).to.be.oneOf([200, 400, 404]);",
									"});",
									"",
									"if (pm.response.code === 200) {",
									"    pm.test(\"Response has indicators\", function () {",
									"        var jsonData = pm.response.json();",
									"        pm.expect(jsonData).to.have.property('indicators');",
									"        pm.expect(jsonData.indicators).to.have.property('rsi');",
									"        pm.expect(jsonData.indicators).to.have.property('sma');",
									"        pm.expect(jsonData.indicators).to.have.property('ema');",
									"    });",
