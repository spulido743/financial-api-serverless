				{
					"name": "Get Real-Time Price - AAPL",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Response has data\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('data');",
									"});",
									"",
									"pm.test(\"Symbol is AAPL\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.data.symbol).to.eql('AAPL');",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "https://rd99h9lf9h.execute-api.us-east-1.amazonaws.com/prod/price/AAPL",
							"protocol": "https",
							"host": [
								"rd99h9lf9h",
								"execute-api",
								"us-east-1",
								"amazonaws",
								"com"
							],
